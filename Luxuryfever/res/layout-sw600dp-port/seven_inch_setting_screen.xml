<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@color/background"
    android:layout_height="match_parent"
    tools:context=".SettingTab">

    <RelativeLayout 
         android:id="@+id/one"
         android:layout_height="80dip"
         android:layout_width="fill_parent"
         android:background="@drawable/topbar">
         
        <ImageButton 
             android:id="@+id/main_menu"
             android:layout_height="65dip"
             android:layout_width="75dip"
             android:contentDescription="@string/brandsfever"
             android:layout_centerInParent="true"
             android:layout_alignParentLeft="true"
             android:layout_marginLeft="18dip"
             android:background="@drawable/menu_bg"/>
        
        <ImageButton 
             android:id="@+id/cart_btn"
             android:layout_height="65dip"
             android:layout_width="75dp"
             android:contentDescription="@string/brandsfever"
             android:layout_centerInParent="true"
             android:layout_alignParentRight="true"
             android:layout_marginRight="10dip"
             android:background="@drawable/cart_btn_bg"/>
         
    </RelativeLayout>
    
     <RelativeLayout 
         android:id="@+id/RL2"
         android:layout_below="@+id/one"
         android:layout_height="fill_parent"
         android:layout_margin="18dp"
         android:layout_width="fill_parent"
         android:background="@android:color/white">
         
         <RelativeLayout 
             android:id="@+id/set_one"
             android:layout_height="75dp"
             android:layout_width="fill_parent">
             
             <TextView 
                 android:id="@+id/setting_tag"
                 android:layout_height="wrap_content"
                 android:layout_width="wrap_content"
                 android:textSize="25sp"
                 android:typeface="sans"
                 android:text="Settings"
                 android:textColor="#ff8e1345"
                 android:layout_alignParentLeft="true"
                 android:layout_marginLeft="50dp"
                 android:layout_centerInParent="true"/>
             
                <View 
                    android:layout_height="fill_parent"
                    android:layout_width="1dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:background="@android:color/darker_gray"
                    android:layout_centerInParent="true"/>
             
             
             <TextView 
                 android:id="@+id/myaccount_tag"
                 android:layout_height="wrap_content"
                 android:layout_width="wrap_content"
                 android:textSize="25sp"
                 android:typeface="sans"
                 android:text="My Account"
                 android:textColor="#ff8e1345"
                 android:layout_alignParentRight="true"
                 android:layout_marginRight="120dp"
                 android:layout_centerInParent="true"/>             
             
          </RelativeLayout>
           <View 
               android:id="@+id/set_once"
               android:layout_below="@+id/set_one"
                 android:layout_height="1dp"
                 android:layout_width="fill_parent"
                 android:layout_margin="10dp"
                 android:background="@android:color/darker_gray"/>
          
         <RelativeLayout 
             android:id="@+id/set_two"
             android:layout_below="@+id/set_once"
             android:layout_height="500dp"
             android:layout_width="fill_parent"
             android:layout_marginTop="10dp">
             
             <LinearLayout 
                 android:layout_height="wrap_content"
                 android:layout_width="210dp"
                 android:orientation="vertical"
                 android:layout_marginLeft="20dp"
                 android:layout_alignParentLeft="true">
                 
                 <ImageButton 
                     android:id="@+id/about_app"
                     android:layout_height="55dp"
                     android:contentDescription="@string/brandsfever"
                     android:layout_width="fill_parent"
                     android:layout_marginTop="15dp"
                     android:background="@drawable/about_app"/>
                 
                 <ImageButton 
                     android:id="@+id/t_c"
                     android:layout_height="55dp"
                     android:contentDescription="@string/brandsfever"
                     android:layout_width="fill_parent"
                     android:layout_marginTop="17dp"
                     android:background="@drawable/terms_condition"/>
                 
                 <ImageButton 
                     android:id="@+id/p_p"
                     android:layout_height="55dp"
                     android:contentDescription="@string/brandsfever"
                     android:layout_width="fill_parent"
                     android:layout_marginTop="17dp"
                     android:background="@drawable/private_policy"/>
                 
                 <Button 
                     android:id="@+id/t2"
                     android:layout_height="30dp"
                     android:layout_width="fill_parent"
                     android:layout_marginTop="30dp"
                     android:text="Share On"
                     android:typeface="sans"
                     android:layout_marginLeft="15dp"
                     android:gravity="left"
                     android:textStyle="bold"
                     android:textColor="#ff8e1345"
                     android:background="@null"/>
                 
                 <View 
                     android:layout_height="1dp"
                     android:layout_width="fill_parent"
                     android:layout_margin="10dp"
                     android:background="@android:color/darker_gray"/>
                 
                 <ImageButton 
                     android:id="@+id/fb_share"
                     android:layout_height="55dp"
                     android:layout_width="fill_parent"
                     android:contentDescription="@string/brandsfever"
                     android:layout_marginTop="17dp"
                     android:background="@drawable/fbse"/>
                 
                 <ImageButton 
                      android:id="@+id/twitter_share"
                     android:layout_height="55dp"
                     android:contentDescription="@string/brandsfever"
                     android:layout_width="fill_parent"
                     android:layout_marginTop="17dp"
                     android:background="@drawable/twse"/>
                 
             </LinearLayout>
             
             <View 
                 android:layout_height="wrap_content"
                 android:layout_width="1dp"
                 android:background="@android:color/darker_gray"
                 android:layout_centerInParent="true"/>
             
             <LinearLayout 
                 android:layout_height="wrap_content"
                 android:layout_width="220dp"
                 android:layout_marginRight="40dp"
                 android:orientation="vertical"
                 android:layout_alignParentRight="true">
                 
                 <ImageButton 
                      android:id="@+id/edit_user_profile"
                     android:layout_height="55dp"
                     android:layout_width="fill_parent"
                     android:contentDescription="@string/brandsfever"
                     android:layout_marginTop="10dp"
                     android:background="@drawable/edit_my_user_profile"/>
                 
                 <ImageButton 
                      android:id="@+id/change_password"
                     android:layout_height="55dp"
                     android:layout_width="fill_parent"
                     android:contentDescription="@string/brandsfever"
                     android:layout_marginTop="17dp"
                     android:background="@drawable/chnage_password"/>
                 
                 <ImageButton 
                      android:id="@+id/order_history"
                     android:layout_height="55dp"
                     android:contentDescription="@string/brandsfever"
                     android:layout_width="fill_parent"
                     android:layout_marginTop="17dp"
                     android:background="@drawable/see_my_order_history"/>
                 <ImageButton 
                      android:id="@+id/already_invited"
                     android:layout_height="55dp"
                     android:contentDescription="@string/brandsfever"
                     android:layout_width="fill_parent"
                     android:layout_marginTop="17dp"
                     android:background="@drawable/already_invited_people"/>
                 
                 <ImageButton 
                      android:id="@+id/my_store_credits"
                     android:layout_height="55dp"
                     android:contentDescription="@string/brandsfever"
                     android:layout_width="fill_parent"
                     android:layout_marginTop="17dp"
                     android:background="@drawable/my_store_credits"/>
                 
                 <ImageButton 
                      android:id="@+id/faq"
                     android:layout_height="55dp"
                     android:contentDescription="@string/brandsfever"
                     android:layout_width="fill_parent"
                     android:layout_marginTop="17dp"
                     android:background="@drawable/faq"/>
                 
             </LinearLayout>             
          </RelativeLayout>
            <View 
                android:id="@+id/set_three"
                android:layout_below="@+id/set_two"
                android:layout_height="1dp"
                android:layout_width="fill_parent"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="20dp"
                android:layout_marginBottom="10dp"
                android:background="@android:color/darker_gray"/>
            
            <Button
                android:id="@+id/call_customer_care"
                android:layout_margin="20dp"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:layout_below="@+id/set_three"
                android:background="@null"
                android:text="Call us: +6588456088"
                android:textSize="25sp"
                 android:typeface="sans"
                 android:textColor="#ff8e1345"/>
         
         <RelativeLayout 
             android:id="@+id/footer_set"
             android:layout_height="90dp"
             android:layout_width="fill_parent"
             android:layout_alignParentBottom="true">
             
             <ImageButton 
                 android:layout_height="90dp"
                 android:contentDescription="@string/brandsfever"
                 android:layout_width="110dp"
                 android:background="@drawable/lo"
                 android:scaleType="fitXY"
                 android:layout_alignParentRight="true"/>
             
         </RelativeLayout>         
     </RelativeLayout>
    
    

</RelativeLayout>
